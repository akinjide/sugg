{
  "node": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": false,
  "maxlen": 1220,
  "curly": true,
  "eqeqeq": false,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "predef": [
    "$",
    "jQuery",
    "_",
    "Firebase",
    "window",
    "moment",
    "inject",
    "angular",
    "module"
  ],
  "globals": {
      "sugg": true
  }
}
